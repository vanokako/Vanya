#include <iostream>
#include <cstdlib>
#include <cstring>
#include "list.h"




using namespace std;



int main ( )
{
    lisp s1, s2;
    cout << "введите list1:" << endl;
    read_lisp (s1);
    cout << "введен list1: " << endl;
    write_lisp (s1);
    cout << endl;
    cout << "Список перевернут: " << endl;
    s2 = rev (s1, NULL);
    write_lisp (s2);
    cout << endl;
    destroy (s2);
    return 0;
}
