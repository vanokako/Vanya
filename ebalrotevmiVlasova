AStack    SEGMENT  STACK
          DW 12 DUP(?)
AStack    ENDS


DATA      SEGMENT

a	DW	-2
b	DW	-3
i	DW	2
k	DW	1
i1	DW	0
i2	DW	0
res	DW	0

DATA      ENDS

; Љ®¤ Їа®Ја ¬¬л

CODE      SEGMENT
          ASSUME CS:CODE, DS:DATA, SS:AStack

Main      PROC  FAR
          push  DS
          sub   AX,AX
          push  AX
          mov   AX,DATA
          mov   DS,AX
		
		mov bx, i
		add bx, i
		add bx, i
		mov ax, bx
		sal ax, 1
		mov dx, a
		cmp dx, b
		jg l1
			
			neg ax
			add ax, 8
			mov i1, ax

			add bx, 6
			mov i2, ax
		
			jmp l2

		l1:
			add bx, i
			neg bx
			add bx, 7
			mov i1, bx

			neg ax
			add ax, 4
			mov i2, ax

		l2:
			mov dx, k
			cmp dx, 0
			jl l3
			mov dx, i1
			sub dx, i2
			cmp dx, 0
			jg l4
			neg dx
			jmp l4

		l3:
			mov dx, i2
			neg dx
			add dx, 10
			cmp i1, dx
			jg great
			jmp l4

		great:
			mov dx, i1
		
		l4:
			mov res, dx
		
		ret

Main ENDP
CODE ENDS
	END Main
			
	
		
